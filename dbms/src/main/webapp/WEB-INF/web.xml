<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>Archetype Created Web Application</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>Login.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>LoginController</servlet-name>
		<servlet-class>com.dbms.controller.LoginController</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>BeachDataController</display-name>
		<servlet-name>BeachDataController</servlet-name>
		<servlet-class>com.dbms.controller.BeachDataController</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>UpdateBeachRecController</display-name>
		<servlet-name>UpdateBeachRecController</servlet-name>
		<servlet-class>com.dbms.controller.UpdateBeachRecController</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>RegisterUserController</display-name>
		<servlet-name>RegisterUserController</servlet-name>
		<servlet-class>com.dbms.controller.RegisterUserController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UpdateReportController</servlet-name>
		<servlet-class>com.dbms.controller.UpdateReportController</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>ViewReportsController</display-name>
		<servlet-name>ViewReportsController</servlet-name>
		<servlet-class>com.dbms.controller.ViewReportsController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>LoginController</servlet-name>
		<url-pattern>/LoginController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>BeachDataController</servlet-name>
		<url-pattern>/BeachDataController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UpdateBeachRecController</servlet-name>
		<url-pattern>/UpdateBeachRecController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RegisterUserController</servlet-name>
		<url-pattern>/RegisterUserController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UpdateReportController</servlet-name>
		<url-pattern>/UpdateReportController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ViewReportsController</servlet-name>
		<url-pattern>/ViewReportsController</url-pattern>
	</servlet-mapping>
</web-app>
